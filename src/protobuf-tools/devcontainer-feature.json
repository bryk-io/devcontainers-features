{
  "name": "Protobuf Tools",
  "id": "protobuf-tools",
  "version": "1.0.0",
  "description": "Tools and plugin useful to compile and work with protocol buffers",
  "options": {
    "bufVersion": {
      "type": "string",
      "enum": [
        "1.9.0",
        "1.8.0"
      ],
      "default": "1.9.0",
      "description": "Buf compiler version"
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/go:1"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "zxh404.vscode-proto3"
      ]
    }
  }
}
